{
  "name": "todo-list",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "node ./server/index.cjs",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0 -c ./config/eslint/eslintrc.cjs",
    "test": "jest --config ./config/jest/jest.config.ts",
    "preview": "vite preview",
    "prepare": "husky install",
    "sb": "storybook dev -p 6006 -c ./config/storybook",
    "build:sb": "storybook build -c ./config/storybook"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.7",
    "hamburger-react": "^2.5.0",
